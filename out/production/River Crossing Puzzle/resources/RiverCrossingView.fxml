<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameEngine">
   <children>
      <Pane prefHeight="372.0" prefWidth="541.0">
          <Button fx:id="saveGame" layoutX="439.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Save Game" />
          <Button fx:id="newGame" layoutX="108.0" layoutY="14.0" mnemonicParsing="false" onAction="#newGame" prefHeight="25.0" prefWidth="88.0" text="New Game" />
          <Button fx:id="undo" layoutX="218.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="30.0" />
          <Button fx:id="reset" layoutX="260.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="21.0" />
          <Button fx:id="redo" layoutX="293.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="30.0" />
          <Button fx:id="instructions" layoutX="12.0" layoutY="14.0" mnemonicParsing="false" onAction="#showInstructions" prefHeight="25.0" prefWidth="88.0" text="Instructions" />
          <Button fx:id="solve" layoutX="14.0" layoutY="329.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Solve" />
         <Group layoutX="12.0" layoutY="62.0">
            <children>
                <Canvas fx:id="canvas" height="260.0" onMouseClicked="#clickSprite" width="507.0" />
            </children>
         </Group>
          <Button fx:id="move" layoutX="432.0" layoutY="329.0" mnemonicParsing="false" onAction="#initiateMovement" prefHeight="25.0" prefWidth="88.0" text="Move" />
          <Label layoutX="24.0" layoutY="45.0" text="Level : " />
          <Label layoutX="451.0" layoutY="45.0" text="Moves :" />
          <Label fx:id="moves" layoutX="499.0" layoutY="45.0" prefHeight="17.0" prefWidth="21.0" text="0" />
          <Label fx:id="story" layoutX="61.0" layoutY="45.0" prefHeight="17.0" prefWidth="61.0" text=" " />
         <Button layoutX="344.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Load Game" />
         <Label fx:id="message" layoutX="108.0" layoutY="333.0" textFill="RED" />
      </Pane>
   </children>
</AnchorPane>
